{
	"name": "cursorsync",
	"displayName": "CursorSync",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.9.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "cursorsync.profileView",
					"name": "CursorSync Profiles"
				}
			]
		},
		"commands": [
			{
				"command": "cursorsync.refreshGistView",
				"title": "Refresh Gist View",
				"icon": "$(refresh)",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.update.deviceLabel",
				"title": "Update Device Label",
				"category": "$(references)CursorSync",
				"icon": "$(key)"
			},
			{
				"command": "cursorsync.update.settingspath",
				"title": "Discover Settings Path",
				"category": "$(cloud-upload-cloud-download)CursorSync",
				"shortTitle": "Discover Settings Path"
			},
			{
				"command": "cursorsync.debug.showsession",
				"title": "CursorSync: Inspect Auth Session"
			},
			{
				"command": "cursorsync.debug.showstate",
				"title": "CursorSync: Inspect Global State Keys"
			},
			{
				"command": "cursorsync.debug.showcontext",
				"title": "CursorSync: Inspect Context Values"
			},
			{
				"command": "cursorsync.reset.state",
				"title": "CursorSync: Reset Global State"
			},
			{
				"command": "cursorsync.syncNow",
				"title": "Sync: Sync Now",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.switchProfile",
				"title": "Sync: Switch Profile",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.createProfile",
				"title": "Sync: Create New Profile",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.deleteProfile",
				"title": "Sync: Delete Profile",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.resetLocalProfile",
				"title": "Debug: Reset Local Profile Record",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.showMenu",
				"title": "Sync: Show Menu",
				"category": "CursorSync"
			},
			{
				"command": "cursorsync.refreshGistView",
				"title": "Sync: Refresh View",
				"category": "CursorSync",
				"icon": "$(refresh)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "cursorsync.refreshGistView",
					"when": "view == cursorsync.gistView",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.100.0",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.25.1",
		"ts-loader": "^9.5.2",
		"typescript": "^5.8.3",
		"webpack": "^5.99.7",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"json5": "^2.2.3"
	}
}

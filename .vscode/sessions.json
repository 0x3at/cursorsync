{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "noClear": false,
  "sessions": {
    "default": [
      {
        "name": "QuickTerm",
        "icon": "terminal-bash",
        "color": "terminal.ansiBrightBlue",
        "shellPath": "/usr/bin/zsh",
        "strictEnv": false,
        "commands": [
          "echo HelloWorld"
        ]
      },
      {
        "name": "webpack:watch",
        "icon": "watch",
        "color": "terminal.ansiBrightGreen",
        "shellPath": "/usr/bin/zsh",
        "strictEnv": false,
        "commands": [
          "npm run watch"
        ]
      }
    ]
  }
}